{
  "scenario": {
    "name": "Normal Traffic Week",
    "description": "Simulates typical traffic over a week in October with realistic event generation.",
    "time_period": {
      "start_time": "2023-10-01T00:00:00Z",
      "end_time": "2023-10-07T23:59:59Z"
    },
    "event_generators": {
      "web_application": {
        "event_types": {
          "page_view": {
            "frequency": "every 5 minutes",
            "volume": 1000
          },
          "button_click": {
            "frequency": "every 10 minutes",
            "volume": 500
          },
          "form_submission": {
            "frequency": "every 30 minutes",
            "volume": 200
          }
        },
        "dependencies": {
          "page_view": {
            "triggers": [
              {
                "source": "api_service",
                "event": "fetch_product_details",
                "volume": 800
              }
            ]
          },
          "button_click": {
            "triggers": [
              {
                "source": "api_service",
                "event": "submit_order",
                "volume": 300
              }
            ]
          }
        }
      },
      "api_service": {
        "event_types": {
          "fetch_product_details": {
            "frequency": "every 5 minutes",
            "volume": 800
          },
          "submit_order": {
            "frequency": "every 10 minutes",
            "volume": 300
          }
        },
        "dependencies": {
          "fetch_product_details": {
            "triggers": [
              {
                "source": "database_service",
                "event": "retrieve_product_stock",
                "volume": 800
              }
            ]
          },
          "submit_order": {
            "triggers": [
              {
                "source": "database_service",
                "event": "update_order_status",
                "volume": 300
              }
            ]
          }
        }
      },
      "database_service": {
        "event_types": {
          "retrieve_product_stock": {
            "frequency": "every 5 minutes",
            "volume": 800
          },
          "update_order_status": {
            "frequency": "every 10 minutes",
            "volume": 300
          }
        }
      }
    }
  }
} 